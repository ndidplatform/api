{
  "name": "ndid-api",
  "version": "7.1.0",
  "description": "",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "build": "rm -rf build/ && npx babel src -d ./build/ --source-maps inline --verbose",
    "start": "npm run build && node build/server.js",
    "test": "npm run build && NODE_ID=unit-test npx mocha 'build/**/*.test.js'",
    "test:es6": "NODE_ID=unit-test mocha 'src/**/*.test.js' --require @babel/register --require source-map-support/register",
    "initDev": "npm run build && NODE_ID=ndid1 node build/dev_init.js && npm run delete-local-data-cache",
    "delete-local-data-cache": "rm -rf data",
    "docker-build": "./docker/build.sh",
    "docker-up": "docker-compose -f docker/docker-compose.yml up",
    "docker-down": "docker-compose -f docker/docker-compose.yml down"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.7",
    "@grpc/proto-loader": "^0.6.9",
    "ajv": "^8.10.0",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "asn1.js": "^5.4.1",
    "base64url": "^3.0.1",
    "bigint-buffer": "^1.1.5",
    "body-parser": "^1.19.2",
    "data-urls": "^3.0.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "grpc": "^1.24.11",
    "ioredis": "^5.0.3",
    "mkdirp": "^1.0.4",
    "morgan": "^1.10.0",
    "mustache": "^4.2.0",
    "ndid-error": "file:../ndid-error",
    "ndid-logger": "file:../ndid-logger",
    "node-fetch": "^2.6.7",
    "prom-client": "^11.2.1",
    "protobufjs": "^6.11.2",
    "simple-backoff": "^1.1.0",
    "source-map-support": "^0.5.21",
    "uuid": "^8.3.2",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.7",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/eslint-plugin": "^7.17.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.17.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.7",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "eslint": "^8.11.0",
    "mocha": "^9.2.2",
    "rewire": "^6.0.0"
  }
}
