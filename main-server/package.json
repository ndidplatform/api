{
  "name": "ndid-api",
  "version": "8.0.1",
  "description": "",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "build": "rm -rf build/ && npx babel src -d ./build/ --source-maps inline --verbose",
    "start": "npm run build && node build/server.js",
    "test": "npm run build && NODE_ID=unit-test npx mocha 'build/**/*.test.js'",
    "test:es6": "NODE_ID=unit-test mocha 'src/**/*.test.js' --require @babel/register --require source-map-support/register",
    "initDev": "npm run build && NODE_ID=ndid1 node build/dev_init.js && npm run delete-local-data-cache",
    "delete-local-data-cache": "rm -rf data",
    "docker-build": "./docker/build.sh",
    "docker-up": "docker-compose -f docker/docker-compose.yml up",
    "docker-down": "docker-compose -f docker/docker-compose.yml down"
  },
  "dependencies": {
    "@babel/runtime": "^7.25.0",
    "@grpc/grpc-js": "^1.11.1",
    "@grpc/proto-loader": "^0.7.12",
    "ajv": "^8.17.1",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "asn1.js": "^5.4.1",
    "base64url": "^3.0.1",
    "bigint-buffer": "^1.1.5",
    "body-parser": "^1.19.2",
    "data-urls": "^5.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "mkdirp": "^3.0.1",
    "morgan": "^1.10.0",
    "mustache": "^4.2.0",
    "ndid-error": "file:../ndid-error",
    "ndid-logger": "file:../ndid-logger",
    "node-fetch": "^2.7.0",
    "prom-client": "^15.1.3",
    "protobufjs": "^7.3.2",
    "simple-backoff": "^1.1.0",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/eslint-plugin": "^7.25.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.25.2",
    "@babel/register": "^7.24.6",
    "chai": "^4.5.0",
    "chai-http": "^4.4.0",
    "eslint": "^8.57.0",
    "mocha": "^10.7.0",
    "rewire": "^7.0.0"
  }
}
