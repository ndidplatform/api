{
  "name": "ndid-api",
  "version": "5.2.0",
  "description": "",
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "build": "rm -rf build/ && npx babel src -d ./build/ --source-maps inline --verbose",
    "start": "npm run build && node build/server.js",
    "test": "npm run build && NODE_ID=unit-test npx mocha 'build/**/*.test.js'",
    "test:es6": "NODE_ID=unit-test mocha 'src/**/*.test.js' --require @babel/register --require source-map-support/register",
    "initDev": "npm run build && NODE_ID=ndid1 node build/dev_init.js && npm run delete-local-data-cache",
    "delete-local-data-cache": "rm -rf data",
    "docker-build": "./docker/build.sh",
    "docker-up": "docker-compose -f docker/docker-compose.yml up",
    "docker-down": "docker-compose -f docker/docker-compose.yml down"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "@grpc/proto-loader": "^0.3.0",
    "ajv": "^6.12.3",
    "ajv-keywords": "^3.5.2",
    "asn1.js": "^5.0.1",
    "base64url": "^3.0.1",
    "bigint-buffer": "^1.1.5",
    "body-parser": "^1.18.2",
    "data-urls": "^1.1.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "grpc": "^1.15.1",
    "ioredis": "^4.6.2",
    "mkdirp": "^0.5.1",
    "morgan": "^1.9.0",
    "mustache": "^2.3.0",
    "ndid-error": "file:../ndid-error",
    "ndid-logger": "file:../ndid-logger",
    "node-fetch": "^2.6.1",
    "prom-client": "^11.2.1",
    "protobufjs": "^6.8.8",
    "simple-backoff": "^1.1.0",
    "source-map-support": "^0.5.19",
    "uuid": "^3.3.2",
    "ws": "^5.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/eslint-plugin": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "eslint": "^7.6.0",
    "mocha": "^7.1.2"
  }
}
